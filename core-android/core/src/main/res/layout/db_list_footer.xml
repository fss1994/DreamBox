<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="60dp"
    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout">

    <ViewStub
        android:id="@+id/view_stub_loading"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout="@layout/db_list_footer_loading" />

    <ViewStub
        android:id="@+id/view_stub_end"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout="@layout/db_list_footer_end" />

    <ViewStub
        android:id="@+id/view_stub_network_error"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout="@layout/db_list_footer_network_error" />

</merge>